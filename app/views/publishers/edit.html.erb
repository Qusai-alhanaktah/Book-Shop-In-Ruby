<h1>Edit Publisher</h1>
<%= form_for @publisher do |f| %>
   <div class="form-group">
        <%= f.label :name %><br/>
        <%= f.text_field :name , class: "form-control" %>
    </div>
        <%= f.submit "Submit", class: "btn btn-primary"  %>
        <%= link_to "Cancel", publishers_path, class: "btn btn-default" %>
<% end %>
